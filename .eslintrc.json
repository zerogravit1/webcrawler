{
    "env": {
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "align": "values",
        "no-bitwise": 2,
        "no-cond-assign": [
          2,
          "except-parens"
        ],
        "complexity": [2, 15],
        "curly": [
          2,
          "all"
        ],
        "semi": [2, "always"],
        "eqeqeq": 2,
        "no-eq-null": 2,
        "no-unused-expressions": 2,
        "guard-for-in": 2,
        "wrap-iife": [2, "inside", { "functionPrototypeMethods": true } ],
        "new-cap": 2,
        "no-caller": 2,
        "no-empty": 2,
        "no-irregular-whitespace": 2,
        "no-new": 2,
        "quotes": [
          2,
          "single",
          {
            "avoidEscape": true
          }
        ],
        "strict": [
          2,
          "global"
        ],
        "dot-notation": 2,
        "no-undef": 2,
        "no-unused-vars": [2, {"vars": "all", "args": "all", "ignoreRestSiblings": false } ],
        "keyword-spacing": [ 2, {} ],
        "space-before-blocks": [
          2,
          "always"
        ],
        "padded-blocks": [
          2,
          "never"
        ],
        "array-bracket-spacing": [
          2,
          "always",
          {
            "singleValue": true,
            "objectsInArrays": false,
            "arraysInArrays": false
          }
        ],
        "space-in-parens": [
          2,
          "always"
        ],
        "comma-style": [
          2,
          "last"
        ],
        "space-unary-ops": [
          2,
          {
            "words": false,
            "nonwords": false
          }
        ],
        "space-infix-ops": 2,
        "camelcase": [
          2,
          {
            "properties": "always"
          }
        ],
        "no-with": 2,
        "no-multi-str": 2,
        "no-multiple-empty-lines": 2,
        "no-trailing-spaces": 2,
        "comma-dangle": [
          2,
          "never"
        ],
        "brace-style": [
          2,
          "1tbs",
          {
            "allowSingleLine": true
          }
        ],
        "eol-last": 2,
        "max-len": [
          2,
          120
        ],
        "key-spacing": [
          2,
          {
            "align": {
              "beforeColon": false,
              "afterColon": true,
              "on": "value"
            }
          }
        ],
        "sort-vars": 2,
        "yoda": [
          2,
          "never"
        ]
      }
}
